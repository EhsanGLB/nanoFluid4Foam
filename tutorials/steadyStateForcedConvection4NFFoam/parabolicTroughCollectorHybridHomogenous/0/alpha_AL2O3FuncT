FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha_AL2O3FuncT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions		[0 0 0 0 0 0 0];

internalField		uniform 0.01;

boundaryField
{
    inlet
    {
	type		fixedValue;
	value		$internalField;
    }

    outlet
    {
	type		zeroGradient;
    }

    tubeWall
    {
	type		zeroGradient;
    }
}
