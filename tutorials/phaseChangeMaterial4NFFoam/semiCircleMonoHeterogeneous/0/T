FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions		[0 0 0 1 0 0 0];

internalField		uniform 273.0;

boundaryField
{
    semiCylindericalWall
    {
	type		robin4NFFoam;
        Kappa           kappaEff;
        qo              (400);
        ho              (7.0);
        To              (273.0);
    }

    ".*"
    {
	type		zeroGradient;
    }
}
