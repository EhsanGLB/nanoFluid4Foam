{
    inletu
    {
        type            fixedValue;
        value           uniform 0;
    }

    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    ".*"
    {
	type		zeroGradient;
    }

    frontAndBack
    {
        type            empty;
        //type            zeroGradient;
    }

    frontAndBacku
    {
        type            empty;
        //type            zeroGradient;
